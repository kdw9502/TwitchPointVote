{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "title": "Twitch.tv 투표 & 추첨"
   },
   "description": "Twitch.tv 투표 & 추첨 프로그램",
   "icons": {
      "128": "icons/128.png",
      "32": "icons/32.png",
      "48": "icons/48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "name": "트위치 투표 추첨기",
   "permissions": [ "*://*.twitch.tv/*", "storage", "tts" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
